{
    "manifest_version": 2,
    "name": "Random Searches Extension",
    "version": "1.0",
    "description": "Perform random searches in Microsoft Edge.",
    "permissions": ["tabs"],
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
  